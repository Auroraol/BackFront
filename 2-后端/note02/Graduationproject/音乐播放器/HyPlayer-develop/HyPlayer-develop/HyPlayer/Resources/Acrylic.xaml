<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:HyPlayer"
    xmlns:media="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
    <media:AcrylicBrush
        x:Key="PlaybackAcrylicInAppLuminosity"
        BackgroundSource="Backdrop"
        FallbackColor="#3E3E3E"
        TintColor="#3E3E3E"
        TintLuminosityOpacity="0.183000008692034"
        TintOpacity="0.67500003206078" />
    <media:AcrylicBrush
        x:Key="AccentAcrylicInAppLuminosity"
        BackgroundSource="Backdrop"
        FallbackColor="SkyBlue"
        TintColor="#FF93CDF1"
        TintLuminosityOpacity="0.653000031015836"
        TintOpacity="0.784000037238002" />
    <media:AcrylicBrush
        x:Key="BackgroundInAppLuminosity"
        BackgroundSource="Backdrop"
        FallbackColor="#2B2B2B"
        TintColor="#2B2B2B"
        TintLuminosityOpacity="0.653000031015836"
        TintOpacity="0.784000037238002" />
    <media:AcrylicBrush
        x:Key="ExpandedPlayerLuminosity"
        BackgroundSource="HostBackdrop"
        FallbackColor="SkyBlue"
        TintColor="#2B2B2B"
        TintLuminosityOpacity="0.653000031015836"
        TintOpacity="0.784000037238002" />
    <media:AcrylicBrush
        x:Key="PlayListItemLuminosity"
        BackgroundSource="Backdrop"
        FallbackColor="SkyBlue"
        TintColor="#3d6276"
        TintLuminosityOpacity="0.528000025078654"
        TintOpacity="0.122000005794689" />
    <media:AcrylicBrush
        x:Key="AccentPanelAcrylic"
        BackgroundSource="HostBackdrop"
        FallbackColor="#2B2B2B"
        TintColor="#4D76B3"
        TintLuminosityOpacity="0.36"
        TintOpacity="0.178000008454546" />
    <AcrylicBrush
        x:Key="NormalWindowBackgroundAcrylic"
        BackgroundSource="HostBackdrop"
        FallbackColor="{ThemeResource SystemRevealAltHighColor}"
        TintColor="{ThemeResource SystemRevealAltHighColor}"
        TintLuminosityOpacity="0.67"
        TintOpacity="0.25" />
    <AcrylicBrush
        x:Key="GridPlayBarBackgroundAcrylic"
        BackgroundSource="HostBackdrop"
        FallbackColor="{ThemeResource SystemRevealAltHighColor}"
        TintColor="{ThemeResource SystemRevealAltHighColor}"
        TintLuminosityOpacity="0.5"
        TintOpacity="0.2" />
    <AcrylicBrush
        x:Key="GridPlayBarBackgroundBlur"
        BackgroundSource="Backdrop"
        FallbackColor="{ThemeResource SystemRevealAltHighColor}"
        TintColor="{ThemeResource SystemRevealAltHighColor}"
        TintLuminosityOpacity="0"
        TintOpacity="0" />
</ResourceDictionary>