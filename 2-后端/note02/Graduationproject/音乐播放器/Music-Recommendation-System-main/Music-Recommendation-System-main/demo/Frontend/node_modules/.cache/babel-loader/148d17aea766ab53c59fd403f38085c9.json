de>\r\n\r\n\t\t<el-container>\r\n\t\t\t<el-header\r\n\t\t\t\tstyle=\"text-align: right; font-size: 15px;position:relative;\"\r\n\t\t\t>\r\n\t\t\t\t<el-dropdown\r\n\t\t\t\t\tstyle=\"position:absolute;top:5px;right:20px;width:100px;height:50px;text-align:center;color:#fff;\"\r\n\t\t\t\t>\r\n\t\t\t\t\t<i\r\n\t\t\t\t\t\tclass=\"el-icon-user-solid\"\r\n\t\t\t\t\t\tstyle=\"margin-right:10px;\"\r\n\t\t\t\t\t></i>\r\n\t\t\t\t\t<span>{{ adminName }}</span>\r\n\t\t\t\t\t<template #dropdown>\r\n\t\t\t\t\t\t<el-dropdown-menu>\r\n\t\t\t\t\t\t\t<el-dropdown-item @click=\"logOut\">\r\n\t\t\t\t\t\t\t\t退出\r\n\t\t\t\t\t\t\t</el-dropdown-item>\r\n\t\t\t\t\t\t</el-dropdown-menu>\r\n\t\t\t\t\t</template>\r\n\t\t\t\t</el-dropdown>\r\n\t\t\t</el-header>\r\n\r\n\t\t\t<el-main>\r\n\t\t\t\t<!-- 统计项 -->\r\n\t\t\t\t<el-table\r\n\t\t\t\t\t:data=\"totalData\"\r\n\t\t\t\t>\r\n                    <el-table-column\r\n\t\t\t\t\t\twidth=\"50\"\r\n\t\t\t\t\t>\r\n                    </el-table-column>\r\n                    <el-table-column\r\n\t\t\t\t\t\talign=\"center\"\r\n\t\t\t\t\t\tprop=\"totalsongs\"\r\n\t\t\t\t\t\tlabel=\"歌曲总数\"\r\n\t\t\t\t\t\twidth=\"200\"\r\n\t\t\t\t\t>\r\n                    \t<template #default=\"scope\">\r\n\t\t\t\t\t\t\t{{ scope.row.totalsongs }}\r\n\t\t\t\t\t\t</template>\r\n                    </el-table-column>\r\n\t\t\t\t\t<el-table-column\r\n\t\t\t\t\t\talign=\"center\"\r\n\t\t\t\t\t\tprop=\"totalusers\"\r\n\t\t\t\t\t\tlabel=\"用户总数\"\r\n\t\t\t\t\t\twidth=\"200\"\r\n\t\t\t\t\t>\r\n                        <template #default=\"scope\">\r\n\t\t\t\t\t\t\t{{ scope.row.totalusers }}\r\n\t\t\t\t\t\t</template>\r\n                    </el-table-column>\r\n\t\t\t\t\t<el-table-column\r\n\t\t\t\t\t\talign=\"center\"\r\n\t\t\t\t\t\tprop=\"totalsheets\"\r\n\t\t\t\t\t\tlabel=\"歌单总数\"\r\n\t\t\t\t\t\twidth=\"200\"\r\n\t\t\t\t\t>\r\n\t\t\t\t\t\t<template #default=\"scope\">\r\n\t\t\t\t\t\t\t{{ scope.row.totalsheets }}\r\n\t\t\t\t\t\t</template>\r\n\t\t\t\t\t</el-table-column>\r\n      