rB,KAAK,CAAC,GAAN,CAAU,Y,GAAY;AAAA;WADD,CACxB;AAAA,SADiB,C;;;;OANnB,CApDkB,CARlB;AAAA,K;;;;GAND,E;;AAAA,I,QAAA,CA6EK,CA9EN,C","sourcesContent":["<template>\n\t<div id=\"users-recommend\">\n\t\t<el-table\n\t\t\t:data=\"usersData\"\n\t\t\tstripe\n\t\t\tstyle=\"width: 100%\"\n\t\t\t:header-cell-style=\"{ fontWeight: 'normal', border: 'none' }\"\n\t\t>\n\t\t\t<el-table-column align=\"center\" prop=\"index\" label=\"\" width=\"80\">\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<span\n\t\t\t\t\t\tstyle=\"font-size:16px;width:20px;display:inline-block;\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.$index + 1 }}\n\t\t\t\t\t</span>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"name\"\n\t\t\t\tlabel=\"姓名\"\n\t\t\t\twidth=\"150\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<span\n\t\t\t\t\t\tstyle=\"cursor:pointer;\"\n\t\t\t\t\t\t@click=\"recommendUserDetail(scope.$index)\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.name }}\n\t\t\t\t\t</span>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"gender\"\n\t\t\t\tlabel=\"性别\"\n\t\t\t\twidth=\"100\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t{{ scope.row.gender }}\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"age\"\n\t\t\t\tlabel=\"年龄\"\n\t\t\t\twidth=\"100\"\n\t\t\t></el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"area\"\n\t\t\t\tlabel=\"所在区域\"\n\t\t\t\twidth=\"150\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t{{ scope.row.area }}\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"des\"\n\t\t\t\tlabel=\"自我简介\"\n\t\t\t\twidth=\"200\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t{{ scope.row.des }}\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"registerTime\"\n\t\t\t\tlabel=\"注册时间\"\n\t\t\t\twidth=\"160\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t{{ scope.row.registerTime }}\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t</el-table>\n\t</div>\n</template>\n\n<script>\nimport { getRecommendUsers } from \"../network/request.js\"\nimport { useStore } from \"vuex\"\nexport default {\n\tname: \"recommendusers\",\n\tdata() {\n\t\tconst item = {\n\t\t\tuid: \"\",\n\t\t\tname: \"\",\n\t\t\tgender: \"\",\n\t\t\tage: \"\",\n\t\t\tarea: \"\",\n\t\t\tdes: \"\",\n\t\t\tregisterTime: \"\",\n\t\t}\n\t\treturn {\n\t\t\tuid: useStore().state.user.uid,\n\t\t\tdownUrl: \"\",\n\t\t\tusersData: [],\n\t\t\ttableData: Array(10).fill(item),\n\t\t}\n\t},\n\tbeforeCreate() {\n\t\tconst user = useStore().state.user\n\t\tconsole.log(user)\n\t\tvar useruid = user.uid\n\t\tconsole.log(useruid)\n\t\tgetRecommendUsers({ uid: useruid + \"\" }).then((response) => {\n\t\t\tthis.usersData = response.data.data\n\t\t\tconsole.log(this.usersData)\n\t\t})\n\t},\n\tcreated() {\n\t\tconst user = useStore().state.user\n\t\tconsole.log(user)\n\t\tvar useruid = user.uid\n\t\tconsole.log(useruid)\n\t\t// 判断是否为游客\n\t\tif(parseInt(useruid) < 100){\n\t\t\tthis.$router.push({\n\t\t\t\tpath: \"/login\",\n\t\t\t})\n\t\t\tthis.$message({\n\t\t\t\ttype: \"info\",\n\t\t\t\tmessage: \"请先登录或注册一个账号\",\n\t\t\t})\n\t\t}\n\t\t// 页面加载时读取数据\n\t\tif (window.sessionStorage.getItem(\"user\")) {\n\t\t\tconsole.log(\"页面加载\")\n\t\t\tconst session_user = JSON.parse(\n\t\t\t\twindow.sessionStorage.getItem(\"user\"),\n\t\t\t)\n\t\t\tconsole.log(session_user)\n\t\t\tthis.$store.commit(\"SET_USER\", session_user)\n\t\t}\n\t\t// 页面刷新时保存状态\n\t\twindow.addEventListener(\"beforeunload\", () => {\n\t\t\tconsole.log(\"页面刷新\")\n\t\t\tconsole.log(this.$store.state.user)\n\t\t\twindow.sessionStorage.setItem(\n\t\t\t\t\"user\",\n\t\t\t\tJSON.stringify(this.$store.state.user),\n\t\t\t)\n\t\t})\n\t},\n\tmethods: {\n\t\t// 下一页\n\t\thandleCurrentChange(val) {\n\t\t\tconsole.log(`当前页: ${val}`)\n\t\t\tthis.currentPage = val\n\t\t\t// 换页之后改变tableData，重新渲染\n\t\t\tthis.usersData = this.usersData.slice(\n\t\t\t\t(this.currentPage - 1) * this.pageSize,\n\t\t\t\tthis.currentPage * this.pageSize,\n\t\t\t)\n\t\t\tconsole.log(this.usersData)\n\t\t},\n\t\t// 跳转到相似用户详情信息页面\n\t\trecommendUserDetail(index) {\n\t\t\tconsole.log(\"选择用户的索引：\" + index)\n\t\t\tconst user = this.usersData[parseInt(index)]\n\t\t\tconsole.log(user)\n\t\t\tthis.$router.push({\n\t\t\t\tname: \"userRecord\",\n\t\t\t\tparams: {\n\t\t\t\t\tuid: user.uid,\n\t\t\t\t\tname: user.name,\n\t\t\t\t\tgender: user.gender,\n\t\t\t\t\tarea: user.area,\n\t\t\t\t\tdes: user.des,\n\t\t\t\t},\n\t\t\t})\n\t\t},\n\t},\n}\n</script>\n\n<style scoped>\n/* 字体图标鼠标覆盖变色 */\n#users-recommend i.iconfont.icon-bofang:hover {\n\tcursor: pointer;\n\tcolor: #f56c6c;\n}\n\n#users-recommend {\n\tpadding-top: 0;\n\tposition: absolute;\n\twidth: 960px !important;\n\tright: 0;\n\tleft: 0;\n\tmargin-left: auto;\n\tmargin-right: auto;\n}\n</style>\n"],"sourceRoot":""}]}